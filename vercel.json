{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "builds": [
    {
      "src": "web/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(_next|static)(.*)",
      "dest": "web/$0"
    },
    {
      "src": "/(favicon.ico|manifest.json|icons/.*)",
      "dest": "web/$0"
    },
    {
      "src": "/(.*)",
      "dest": "web/$1"
    }
  ]
}
